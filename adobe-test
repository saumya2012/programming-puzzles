var arr = ['a','b','c','d','e','f','g','h','i','j','k','1','2','3','4','5','6','7','8','a','b','c','d','e','f','g','h','i','j','k','1','2','3','4','5','6','7'];
var len = 37;
var output = [];

function MyPrint (arr, len) {
  output.push(arr[2]);
  for (var i=-1; i<len; ) {
    i = i+4;
    if (arr[i]) {
      output.push(arr[i]);  
    }
    i = i+3;
    if (arr[i]) {
      output.push(arr[i]);  
    }
    
  } 
  console.log('output', output)
}
MyPrint(arr, len);
